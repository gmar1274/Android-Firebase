<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <fragment
        android:layout_marginTop="8dp"
        android:layout_marginBottom="8dp"
        android:id="@+id/place_autocomplete_fragment"
        android:layout_width="match_parent"
        android:layout_height="17dp"
        android:name="com.google.android.gms.location.places.ui.PlaceAutocompleteFragment"
        tools:layout_editor_absoluteY="0dp"
        tools:layout_editor_absoluteX="0dp" />

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <TextView
            android:layout_marginLeft="32dp"
            android:text="Day"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView22"
            android:textStyle="normal|bold"
            android:textColor="@color/cardview_dark_background"
            android:textSize="24sp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true" />

        <TextView
            android:text="CLOSE"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView28"
            app:layout_constraintLeft_toRightOf="@+id/textView27"
            app:layout_constraintTop_toTopOf="parent"
            android:textSize="24sp"
            android:textStyle="normal|bold"
            android:textColor="@color/cardview_dark_background"
            android:layout_alignParentTop="true"
            android:layout_alignRight="@+id/spinner_mon_close"
            android:layout_alignEnd="@+id/spinner_mon_close"
            android:layout_alignLeft="@+id/spinner_mon_close"
            android:layout_alignStart="@+id/spinner_mon_close"
            android:textAlignment="center" />

        <TextView
            android:layout_marginLeft="50dp"
            android:text="OPEN"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView27"
            app:layout_constraintLeft_toRightOf="@+id/textView22"
            android:textSize="24sp"
            android:textStyle="normal|bold"
            android:textColor="@color/cardview_dark_background"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_alignParentTop="true"
            android:layout_toEndOf="@+id/textView31"
            android:layout_toRightOf="@+id/textView31" />

        <TextView
            android:layout_marginTop="16dp"

            android:text="Tuesday"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignRight="@+id/textView29"
            android:layout_alignEnd="@+id/textView29"
            android:id="@+id/textView30"
            android:layout_below="@+id/textView29"
            android:textSize="18sp" />

        <TextView
            android:layout_marginTop="16dp"

            android:text="Wednesday"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/textView30"
            android:layout_alignLeft="@+id/textView30"
            android:layout_alignStart="@+id/textView30"
            android:id="@+id/textView31"
            android:textSize="18sp" />

        <TextView
            android:text="Monday"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView29"
            tools:layout_editor_absoluteX="0dp"
            app:layout_constraintTop_toBottomOf="@+id/textView22"
            android:layout_below="@+id/textView22"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_marginTop="16dp"

            android:layout_marginLeft="17dp"
            android:layout_marginStart="17dp"
            android:textSize="18sp" />

        <TextView
            android:layout_marginTop="16dp"

            android:text="Thursday"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/textView31"
            android:layout_alignRight="@+id/textView31"
            android:layout_alignEnd="@+id/textView31"
            android:id="@+id/textView32"
            android:layout_alignLeft="@+id/textView31"
            android:layout_alignStart="@+id/textView31"
            android:textSize="18sp" />

        <TextView
            android:layout_marginTop="16dp"

            android:text="Friday"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/textView32"
            android:layout_alignRight="@+id/textView32"
            android:layout_alignEnd="@+id/textView32"
            android:id="@+id/textView33"
            android:layout_alignLeft="@+id/textView29"
            android:layout_alignStart="@+id/textView29"
            android:textSize="18sp" />

        <TextView
            android:text="Saturday"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/textView33"
            android:layout_alignLeft="@+id/textView33"
            android:layout_alignStart="@+id/textView33"
            android:layout_marginTop="16dp"

            android:id="@+id/textView34"
            android:textSize="18sp" />

        <TextView
            android:layout_marginTop="16dp"

            android:text="Sunday"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/textView34"
            android:layout_alignLeft="@+id/textView34"
            android:layout_alignStart="@+id/textView34"
            android:id="@+id/textView35"
            android:textSize="18sp" />

        <Spinner
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/spinner_mon_open"
            android:layout_alignTop="@+id/textView29"
            android:layout_toRightOf="@+id/textView31"
            android:layout_toEndOf="@+id/textView31" />

        <Spinner
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/spinner_tues_open"
            android:layout_alignBaseline="@+id/textView30"
            android:layout_alignBottom="@+id/textView30"
            android:layout_alignLeft="@+id/spinner_mon_open"
            android:layout_alignStart="@+id/spinner_mon_open" />

        <Spinner
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/spinner_wed_open"
            android:layout_alignBottom="@+id/textView31"
            android:layout_toRightOf="@+id/textView31"
            android:layout_toEndOf="@+id/textView31" />

        <Spinner
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/spinner_thurs_open"
            android:layout_alignBottom="@+id/textView32"
            android:layout_toRightOf="@+id/textView32"
            android:layout_toEndOf="@+id/textView32" />

        <Spinner
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/spinner_fri_open"
            android:layout_alignBottom="@+id/textView33"
            android:layout_toRightOf="@+id/textView33"
            android:layout_toEndOf="@+id/textView33" />

        <Spinner
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/spinner_sat_open"
            android:layout_alignBottom="@+id/textView34"
            android:layout_toRightOf="@+id/textView33"
            android:layout_toEndOf="@+id/textView33" />

        <Spinner
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/spinner_sun_open"
            android:layout_alignBottom="@+id/textView35"
            android:layout_alignLeft="@+id/spinner_sat_open"
            android:layout_alignStart="@+id/spinner_sat_open" />

        <Spinner
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/spinner_wed_close"
            android:layout_alignBottom="@+id/spinner_wed_open"
            android:layout_alignLeft="@+id/spinner_tues_close"
            android:layout_alignStart="@+id/spinner_tues_close" />

        <Spinner
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/spinner_thurs_close"
            android:layout_alignBaseline="@+id/textView32"
            android:layout_alignBottom="@+id/textView32"
            android:layout_alignLeft="@+id/spinner_wed_close"
            android:layout_alignStart="@+id/spinner_wed_close" />

        <Spinner
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/spinner_fri_close"
            android:layout_alignBaseline="@+id/spinner_fri_open"
            android:layout_alignBottom="@+id/spinner_fri_open"
            android:layout_alignLeft="@+id/spinner_thurs_close"
            android:layout_alignStart="@+id/spinner_thurs_close" />

        <Spinner
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/spinner_mon_close"
            android:layout_marginLeft="72dp"
            android:layout_marginStart="72dp"
            android:layout_alignBaseline="@+id/textView29"
            android:layout_alignBottom="@+id/textView29"
            android:layout_toRightOf="@+id/spinner_mon_open"
            android:layout_toEndOf="@+id/spinner_mon_open" />

        <Spinner
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/spinner_sat_close"
            android:layout_alignBaseline="@+id/spinner_sat_open"
            android:layout_alignBottom="@+id/spinner_sat_open"
            android:layout_alignLeft="@+id/spinner_fri_close"
            android:layout_alignStart="@+id/spinner_fri_close" />

        <Spinner
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/spinner_sun_close"
            android:layout_alignBaseline="@+id/spinner_sun_open"
            android:layout_alignBottom="@+id/spinner_sun_open"
            android:layout_alignLeft="@+id/spinner_sat_close"
            android:layout_alignStart="@+id/spinner_sat_close" />

        <Spinner
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/spinner_tues_close"
            android:layout_above="@+id/textView31"
            android:layout_alignLeft="@+id/spinner_mon_close"
            android:layout_alignStart="@+id/spinner_mon_close" />

    </RelativeLayout>


</LinearLayout>